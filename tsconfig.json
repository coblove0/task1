{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"],
      "@/*": ["./src/*"]
    },
    "esModuleInterop": true
  }
}
